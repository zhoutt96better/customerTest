tags: 
  DV: &DV
    - tag: "DV"
      tagstyle: "is-success"
      name: "Desarrollo (DV)"
  PY: &PY
    - tag: "PY"
      tagstyle: "is-warning"
      name: "Prototipo (PY)"
  PD: &PD
    - tag: "PD"
      tagstyle: "is-danger"  
      name: "Producción (PD)"
  SM: &SM
    - tag: "SM"
      tagstyle: "is-info"
      name: "Server Manager (Cachés)"
  ALB: &ALB
    - subtitle: "ALB - Grupo Albacora S.A."
      target: "_blank"
  ATTINA: &ATTINA
    - subtitle: "ATTINA - Danokar SL"
      target: "_blank"
  BBI: &BBI
    - subtitle: "BBI - Beretta Benelli Ibérica"
      target: "_blank"
  IMQ: &IMQ
    - subtitle: "IMQ - Igualatorio Médico Quirúrgico"
      target: "_blank"
  JUNO: &JUNO
    - subtitle: "JUNO - Industrias Juno"
      target: "_blank"
  MBA: &MBA
    - subtitle: "MBA - MBA Incorporado"
      target: "_blank"
  VS: &VS
    - subtitle: "VS - Valvospain Industrial"
      target: "_blank"


